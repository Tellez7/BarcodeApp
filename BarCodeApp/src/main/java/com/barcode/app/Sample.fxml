<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.barcode.app.FxController">
	<center>
		<VBox prefHeight="415.0" prefWidth="434.0" BorderPane.alignment="CENTER">
			<children>
				<Label fx:id="lblTitle" prefHeight="17.0" prefWidth="95.0" text="Codigo de barras" />
				<TextField fx:id="txtFieldSearch" />
				<Button mnemonicParsing="false" onAction="#onShow" text="Buscar" />
				<Text fx:id="textProduct" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
				<Text fx:id="textProvider" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <ComboBox fx:id="combobox" prefWidth="150.0" />
			</children>
		</VBox>
	</center>
</BorderPane>
